{
    "version": "0.2.0",
    "projectName": "HTNB32L-XXX-OpenCPU",
    "configurations": [
        {
            "name": "Debug - MQTT_Example",
            "cwd": "${workspaceRoot}",
            "executable": "${workspaceRoot}/Build/MQTT_Example/HTNB32L-XXX-MQTT_Example.elf",
            "request": "launch",
            "type": "cortex-debug",
            "servertype": "jlink",
            "device": "CORTEX-M3",
            "interface": "swd",
            "runToEntryPoint": "main_entry",
            "preLaunchTask": "Debug - MQTT_Example", // configured in tasks.json
            // "preLaunchCommands": ["Build all"], // you can execute command instead of task
            // "svdFile": "${workspaceRoot}/Debug/SVD/HTNB32L-XXX.svd", // Include svd to watch device peripherals
            "swoConfig":
            {
                "enabled": true,
                "cpuFrequency": 160000000,
                "swoFrequency": 4000000,
                "source": "probe",
                "decoders":
                [
                    {
                        "label": "ITM port 0 output",
                        "type": "console",
                        "port": 0,
                        "showOnStartup": true,
                        "encoding": "ascii"
                    }
                ]
            }
        },
